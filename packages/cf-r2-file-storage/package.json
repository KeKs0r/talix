{
    "name": "cf-r2-file-storage",
    "version": "0.0.0",
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20221111.1",
        "@types/node": "^18.11.18",
        "eslint-config-custom": "workspace:*",
        "tsconfig": "workspace:*",
        "typescript": "^4.9.4",
        "vite": "^3.2.4",
        "vite-node": "^0.27.0",
        "vitest": "^0.25.6"
    },
    "main": "./src/index.ts",
    "private": true,
    "engines": {
        "node": "18"
    },
    "scripts": {
        "type-check": "pnpm run type-check:app && pnpm run type-check:test",
        "type-check:app": "tsc --noEmit",
        "type-check:test": "tsc --noEmit --project ./tsconfig.spec.json",
        "dev": "vite-node src/dev.ts",
        "lint": "eslint",
        "test": "NODE_OPTIONS=--experimental-vm-modules vitest run",
        "test:watch": "NODE_OPTIONS=--experimental-vm-modules vitest .",
        "repl": "vite-node"
    }
}
